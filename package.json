    "react-intl": "^5.20.3",
    "react-router-dom": "^5.1.2",
    "react-swipeable-views": "^0.14.0",
    "styled-components": "^5.3.0",
    "typescript": "^4.3.4",
    "use-debounce": "^7.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
